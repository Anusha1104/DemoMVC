@model IEnumerable<DemoMVC.Models.EMPDATA>
@{
    ViewBag.Title = "getDeptData";
}

<h2>getDeptData</h2>
@using (Html.BeginForm("GetDept","Database"))
{
<h3>Enter the Deptno: @Html.TextBox("txtDeptno",Request["txtDeptno"])
    <input type="submit" value="GetData"/></h3>

    if(Model!=null)
    {
        <table>
            <tr>
                <td>Empno</td>
                <td>Ename</td>
                <td>Job</td>
                <td>Manager Id</td>
                <td>HireDate</td>
                <td>Salary</td>
                <td>Commission</td>
                <td>Deptno</td>
            </tr>
            @foreach(var E in Model)
            {
        <tr>
            <td>@E.EMPNO</td>
            <td>@E.ENAME</td>
            <td>@E.JOB</td>
            <td>@E.MGR</td>
            <td>@E.HIREDATE</td>
            <td>@E.SAL</td>
            <td>@E.COMM</td>
            <td>@E.DEPTNO</td>
        </tr>
            }
        </table>
    }
}