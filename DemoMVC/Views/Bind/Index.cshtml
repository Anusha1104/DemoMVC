@model DemoMVC.Models.EMPDATA
@{
    ViewBag.Title = "Index";
}
    <script src="~/Scripts/Myscript.js"></script>
<h2>Index</h2>
@using (Html.BeginForm("Update","Bind"))
{
    <table>
        <tr>
            <td>Empno</td>
            <td>@Html.TextBoxFor(a => a.EMPNO,"",new { onblur="func()"})</td>
        </tr>
        <tr>
            <td>Ename</td>
            <td>@Html.TextBoxFor(a => a.ENAME)</td>
        </tr>
        <tr>
            <td>Job</td>
            <td>@Html.TextBoxFor(a => a.JOB)</td>
        </tr>
        <tr>
            <td>Mgr id</td>
            <td>@Html.TextBoxFor(a => a.MGR)</td>
        </tr>
        <tr>
            <td>HIREDATE</td>
            <td>@Html.TextBoxFor(a => a.HIREDATE)</td>
        </tr>
        <tr>
            <td>Salary</td>
            <td>@Html.TextBoxFor(a => a.SAL)</td>
        <tr>
            <td>Comm</td>
            <td>@Html.TextBoxFor(a => a.COMM)</td>
        </tr>

        <tr>
            <td>Deptno</td>
            <td>@Html.TextBoxFor(a => a.DEPTNO)</td>
        </tr>
        <tr>
            <td><input type="submit" value="Edit" /></td>

        </tr>


    </table>

    if(ViewBag.msg!=null)
    {
        <h2>@ViewBag.msg</h2>
    }
}
